{
    "name": "custplace/request-logger",
    "description": "PHP package to log request for a Laravel application on MongoDB",
    "keywords": ["laravel", "api", "logger", "request", "request logger", "mongodb"],
    "license": "MIT",
    "authors": [
        {
            "name": "Younes Adounis",
            "email": "y.adounis@gmail.com"
        },
        {
            "name": "Mohamed Es-seddik",
            "email": "mohamedes691@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Custplace\\requestLogger\\": "src/",
            "Custplace\\requestLogger\\Tests\\": "tests",
            "Custplace\\requestLogger\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Custplace\\requestLogger\\Providers\\requestLogProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.24"
    },
    "require": {
        "jenssegers/mongodb": "^3.8"
    }
}
